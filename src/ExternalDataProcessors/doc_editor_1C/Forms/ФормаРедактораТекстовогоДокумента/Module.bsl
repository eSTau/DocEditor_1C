		Если СвойствоОбъекта.Ключ = "Описание" Или СтрНайти(СвойствоОбъекта.Ключ, "Группа") Тогда
			Продолжить;
		КонецЕсли;
		
		СвойстваОбъекта[СвойствоОбъекта.Ключ] = ПредставлениеСвойстваОбъекта(СвойствоОбъекта.Значение);
		
	КонецЦикла;
	
